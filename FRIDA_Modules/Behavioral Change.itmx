				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>2458.91821961224</eqn>
				<range min="2300" max="2500"/>
				<units>kCal/Person/Day</units>
			</aux>
			<aux name="real GDP per person 1" access="output" throughput="true" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>bc$/Mp/year</units>
			</aux>
			<aux name="effect of GDP per capita\non demand">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(real_GDP_per_person/INIT(real_GDP_per_person))^elasticity_of_demand_to_GDP</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="elasticity of demand\nto GDP">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>0.300930293841348</eqn>
				</element>
				<range min="0" max="1"/>
				<units>dmnl</units>
			</aux>
			<aux name="real GDP per person">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>real_GDP_per_person_1*&quot;UNIT_c$_per_billion_c$&quot;/UNIT_p_per_Mp</eqn>
				<units>c$/person/year</units>
			</aux>
			<aux name="reference GDP per person\nfor socioeconomic effect">
				<doc>${Assumption}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>50000</eqn>
				<range min="45000" max="55000"/>
				<units>c$/person/year</units>
			</aux>
			<aux name="relative GDP per person">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>real_GDP_per_person/reference_GDP_per_person_for_socioeconomic_effect</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="socioeconomic effect\non weight of accessibility">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>minimum_socioeconomic_effect + (maximum_socioeconomic_effect-minimum_socioeconomic_effect)/(1+EXP(growth_rate_of_socioeconomic_effect*(inflection_point_of_relative_GDP-relative_GDP_per_person)))</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="reference weight\nof accessibility">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>0.333333333333</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="inflection point of\nrelative GDP">
				<eqn>1</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="growth rate of\nsocioeconomic effect">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>-5</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="maximum\nsocioeconomic effect">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>1.5</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="minimum\nsocioeconomic effect">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>0.5</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="effect of relative long term consumption on social value perception">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>1 + sensitivity_of_social_value_perception*(perceived_consumption_relative_to_long_term_average-1)</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="sensitivity of social value perception">
				<doc>${Internal Calibration Parameter}
Calibration Parameter: fertilizer use &amp; crop production</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>1.15427559334921</eqn>
				</element>
				<range min="0.1" max="2"/>
				<units>dmnl</units>
			</aux>
			<aux name="tolerance of healthy reference\ndaily consumption per capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>100</eqn>
				<units>kCal/Person/Day</units>
			</aux>
			<aux name="minimum required\ndaily consumption per capita">
				<eqn>2000</eqn>
				<units>kCal/Person/Day</units>
			</aux>
