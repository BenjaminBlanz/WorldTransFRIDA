	<model name="Total_Food_Demand">
		<variables>
			<flow name="change in desired demand per capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(indicated_average_desired_demand_per_capita-Desired_Average_Daily_Demand_Per_Capita)/time_to_adjust_consumption_pattern</eqn>
				<units>kcal/person/day/Year</units>
			</flow>
			<stock name="Desired Average Daily\nDemand Per Capita" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_daily_demand_per_capita_in_1980</eqn>
				<inflow>change_in_desired_demand_per_capita</inflow>
				<units>kcal/person/day</units>
			</stock>
			<aux name="UNIT cal per pcal">
				<doc>${Unit}
</doc>
				<eqn>1000000000000</eqn>
				<units>KCal/PCal</units>
			</aux>
			<aux name="average yearly\ndemand per capita" access="output" isee:autocreated="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Average_Daily_Demand_Per_Capita*UNIT_days_per_year*UNIT_p_per_Mp/UNIT_cal_per_pcal</eqn>
				<units>PCal/Mp/Year</units>
			</aux>
			<aux name="UNIT days per year">
				<doc>${Unit}
</doc>
				<eqn>365</eqn>
				<units>days/year</units>
			</aux>
			<aux name="UNIT p per Mp">
				<doc>${Unit}
</doc>
				<eqn>1000000</eqn>
				<units>person/Mp</units>
			</aux>
			<aux name="time to adjust\nconsumption pattern">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>1.03783526320522</eqn>
				</element>
				<range min="1" max="10"/>
				<units>years</units>
			</aux>
			<aux name="total yearly\nconsumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MIN(total_yearly_demand, total_food_supply)</eqn>
				<units>PCal/Year</units>
			</aux>
			<aux name="average daily\nconsumption per capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>total_yearly_consumption/Population_1/UNIT_days_per_year/UNIT_p_per_Mp*UNIT_cal_per_pcal</eqn>
				<units>kcal/person/day</units>
			</aux>
			<stock name="Perceived Average\nConsumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_daily_consumption_per_capita</eqn>
				<inflow>updating_perceived_consumption</inflow>
				<units>kcal/person/day</units>
			</stock>
			<flow name="updating perceived\nconsumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(average_daily_consumption_per_capita-Perceived_Average_Consumption)/time_to_perceive_average_consumption</eqn>
				<units>kcal/person/day/Year</units>
			</flow>
			<aux name="time to perceive\naverage consumption">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>3.07499274061148</eqn>
				</element>
				<range min="1" max="5"/>
				<units>year</units>
			</aux>
			<stock name="Reference\nShort Term\nConsumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Perceived_Average_Consumption</eqn>
				<inflow>updating_short_term_reference_consumption</inflow>
				<units>kcal/person/day</units>
			</stock>
			<flow name="updating short term\nreference consumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Perceived_Average_Consumption-Reference_Short_Term_Consumption)/time_horizon_for_short_term_perception</eqn>
				<units>kcal/person/day/Year</units>
			</flow>
			<aux name="time horizon for\nshort term perception">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>3.06033895681664</eqn>
				</element>
				<range min="1" max="20"/>
				<units>year</units>
			</aux>
			<stock name="Reference\nLong Term\nConsumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Perceived_Average_Consumption</eqn>
				<inflow>updating_long_term_reference_consumption</inflow>
				<units>kcal/person/day</units>
			</stock>
			<flow name="updating long term\nreference consumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Perceived_Average_Consumption-Reference_Long_Term_Consumption)/time_horizon_for_long_term_perception</eqn>
				<units>kcal/person/day/Year</units>
			</flow>
			<aux name="time horizon for\nlong term perception">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>55.059566262194</eqn>
				</element>
				<range min="20" max="100"/>
				<units>year</units>
			</aux>
			<aux name="indicated short term\ntrend">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Perceived_Average_Consumption-Reference_Short_Term_Consumption)/Reference_Short_Term_Consumption/time_horizon_for_short_term_perception</eqn>
				<units>dmnl/year</units>
			</aux>
			<aux name="desired demand\nfrom descriptive norm">
				<doc>${Explanatory Variable}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Desired_Average_Daily_Demand_Per_Capita*(1+ indicated_short_term_trend*expectation_time_horizon)</eqn>
				<units>kcal/person/day</units>
			</aux>
			<aux name="total food supply">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>animal_products_production_1+crop_supply_for_vegetal_products_1</eqn>
				<units>PCal/Year</units>
			</aux>
			<aux name="food scarcity">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>total_food_supply/total_yearly_demand</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="time to perceive\nchange in accessibility">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>1.59955263703177</eqn>
				</element>
				<range min="1" max="5"/>
				<units>years</units>
			</aux>
			<aux name="perceived consumption\nrelative to long term average">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Perceived_Average_Consumption/Reference_Long_Term_Consumption</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="desired demand\nfrom personal norm">
				<doc>${Explanatory Variable}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MAXSOFT(Desired_Average_Daily_Demand_Per_Capita*effect_of_climate_risk_on_personal_norm*effect_of_consumption_risk_on_personal_norm*effect_of_social_value_on_personal_norm,  minimum_required_daily_consumption_per_capita,  tolerance_of_healthy_reference_daily_consumption_per_capita)</eqn>
				<units>kcal/person/day</units>
			</aux>
			<aux name="sensitivity of personal norm\nto climate risk">
				<doc>${Internal Calibration Parameter}
Calibration Parameter: fertilizer use &amp; crop production</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>-0.160565437339649</eqn>
				</element>
				<range min="-2" max="-0.01"/>
				<units>dmnl</units>
			</aux>
			<aux name="sensitivity of personal norm\nto social value">
				<doc>${Internal Calibration Parameter}
Calibration Parameter: fertilizer use &amp; crop production</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>1.14617265719234</eqn>
				</element>
				<range min="0.1" max="2"/>
				<units>dmnl</units>
			</aux>
			<aux name="expectation\ntime horizon">
				<eqn>1</eqn>
				<units>year</units>
			</aux>
			<aux name="healthy reference\ndaily consumption\nper capita">
				<doc>${External Parameter}
Planetary health diet from EAT Lancet https://www.thelancet.com/journals/lancet/article/PIIS0140-6736(18)31788-4/fulltext#seccestitle80
</doc>
				<eqn>2500</eqn>
				<units>kcal/person/day</units>
			</aux>
			<aux name="consumption relative\nto healthy reference">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_daily_consumption_per_capita/healthy_reference_daily_consumption_per_capita</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="perceived\nconsumption risk">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SMTH1(consumption_relative_to_healthy_reference,  time_to_observe_consequences, consumption_relative_to_healthy_reference)</eqn>
				<isee:delay_aux/>
				<units>dmnl</units>
			</aux>
			<aux name="time to observe\nconsequences">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>5.51122145506475</eqn>
				</element>
				<range min="5" max="30"/>
				<units>years</units>
			</aux>
			<aux name="sensitivity of personal norm\nto consumption risk">
				<doc>${Internal Calibration Parameter}
Calibration Parameter: fertilizer use &amp; crop production</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>-0.818082562347266</eqn>
				</element>
				<range min="-2" max="-0.1"/>
				<units>dmnl</units>
			</aux>
			<aux name="Population 1" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
			<aux name="animal products production 1" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>PCal/Year</units>
			</aux>
			<aux name="effect of climate\nrisk on personal norm">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>perceived_climate_risk^(sensitivity_of_personal_norm_to_climate_risk)</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="effect of consumption\nrisk on personal norm">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>perceived_consumption_risk^sensitivity_of_personal_norm_to_consumption_risk</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="effect of social value\non personal norm">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Perceived_Social_Value^sensitivity_of_personal_norm_to_social_value</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="perceived climate risk" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>dmnl</units>
			</aux>
			<stock name="Perceived\nSocial Value">
				<doc>${Explanatory Variable}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>indicated_social_value</eqn>
				<inflow>change_in_value_perception</inflow>
				<units>dmnl</units>
			</stock>
			<flow name="change in value perception">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(indicated_social_value-Perceived_Social_Value)/time_to_adjust_value_perception</eqn>
				<units>dmnl/year</units>
			</flow>
			<aux name="indicated social value">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>initial_perceived_social_value*effect_of_relative_long_term_consumption_on_social_value_perception*media_influence_multiplier</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="time to adjust\nvalue perception">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>2.34181234469972</eqn>
				</element>
				<range min="1" max="10"/>
				<units>years</units>
			</aux>
			<aux name="UNIT c$ per billion c$">
				<doc>${Unit}
</doc>
				<eqn>1000000000</eqn>
				<units>c$/bc$</units>
			</aux>
			<aux name="sensitivity of demand\nto scarcity">
				<doc>${Internal Calibration Parameter}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<element subscript="1">
					<eqn>0.94062237924441</eqn>
				</element>
				<range min="0" max="1"/>
				<units>dmnl</units>
			</aux>
			<aux name="effect of\nrelative scarcity\non demand">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>food_scarcity^sensitivity_of_demand_to_scarcity</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="initial perceived\nsocial value">
				<eqn>1</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="media influence\nmultiplier">
				<doc>${Assumption}
</doc>
				<eqn>1</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="producer waste fraction">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(total_food_supply-total_yearly_consumption)/total_food_supply</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="indicated\naverage desired demand per capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MIN((weight_of_personal_norm*desired_demand_from_personal_norm + desired_demand_from_descriptive_norm*weight_of_descriptive_norm + weight_of_accessibility*desired_demand_from_accessibility), desired_demand_from_accessibility)</eqn>
				<units>kcal/person/day</units>
			</aux>
			<aux name="indicated demand\nfrom accessibility">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_daily_demand_per_capita_in_1980*effect_of_relative_scarcity_on_demand*effect_of_GDP_per_capita_on_demand</eqn>
				<units>kcal/person/day</units>
			</aux>
			<flow name="change in desired\ndemand from accessibility">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(indicated_demand_from_accessibility-desired_demand_from_accessibility)/time_to_perceive_change_in_accessibility</eqn>
				<units>kcal/person/day/Year</units>
			</flow>
			<aux name="weight of\naccessibility">
				<doc>${Assumption}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>reference_weight_of_accessibility*socioeconomic_effect_on_weight_of_accessibility</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="weight of\npersonal norm">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(1-weight_of_accessibility)*relative_weight_of_personal_norm</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="weight of\ndescriptive norm">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(1-weight_of_accessibility)*(1-relative_weight_of_personal_norm)</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="relative weight\nof personal norm">
				<doc>${Assumption}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>0.5</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="total yearly demand">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_yearly_demand_per_capita*Population_1</eqn>
				<units>PCal/Year</units>
			</aux>
			<aux name="Average Daily\nDemand Per Capita" access="output" isee:autocreated="true">
				<doc>${Calibrated Output,Explanatory Variable}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MIN(Desired_Average_Daily_Demand_Per_Capita, desired_demand_from_accessibility)</eqn>
				<units>kcal/person/day</units>
			</aux>
			<stock name="desired demand\nfrom accessibility">
				<doc>${Explanatory Variable}
</doc>
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>average_daily_demand_per_capita_in_1980</eqn>
				<inflow>change_in_desired_demand_from_accessibility</inflow>
				<units>kcal/person/day</units>
			</stock>
			<aux name="crop supply\nfor vegetal products 1" access="input">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>PCal/Year</units>
			</aux>
